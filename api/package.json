{
    "name": "az-marketplace-sandbox",
    "version": "0.1.0",
    "description": "",
    "main": "index.js",
    "scripts": {
        "compile": "tsc && npm run-script swagger && npm run-script copy-assets",
        "debug": "npm run-script compile && NODE_ENV=development node --inspect-brk --nolazy bin/index.js",
        "debug-nodemon": "nodemon -e ts --exec \"npm run-script debug\"",
        "start": "node bin/index.js",
        "test": "mocha -r ts-node/register --require ./tests/index.ts --reporter-options mochaFile=testReport/test-results.xml --reporter mocha-junit-reporter tests/**/*.ts",
        "test-debug": "mocha -r ts-node/register --require ./tests/index.ts --inspect-brk tests/**/*.ts",
        "test-local": "mocha -r ts-node/register --require ./tests/index.ts tests/**/*.ts",
        "test-mutation": "stryker run",
        "coverage": "tsc && nyc --all --report-dir testReport --reporter cobertura npm run test",
        "coverage-local": "tsc && nyc --all --report-dir testReport --reporter text npm run test-local",
        "swagger": "swagger-jsdoc -d swaggerDef.js -o bin/swagger.json",
        "copy-assets": "cpx 'src/assets/**' 'bin/assets'"
    },
    "dependencies": {
        "@types/mongoose": "^5.7.21",
        "body-parser": "1.18.2",
        "chance": "^1.1.6",
        "class-validator": "^0.12.2",
        "compression": "1.7.1",
        "cookie-parser": "1.4.3",
        "cors": "2.8.5",
        "dotenv": "^8.2.0",
        "express": "^4.17.1",
        "helmet": "3.9.0",
        "mongoose": "^5.9.17",
        "nodemon": "^2.0.4",
        "reflect-metadata": "^0.1.13",
        "request": "^2.88.0",
        "request-promise": "^4.2.5",
        "sinon-mongoose": "^2.3.0",
        "swagger-jsdoc": "3.2.7",
        "swagger-ui-express": "^4.1.4",
        "tsyringe": "^4.0.1",
        "winston": "^3.2.1"
    },
    "devDependencies": {
        "@stryker-mutator/core": "^2.4.0",
        "@stryker-mutator/html-reporter": "^2.4.0",
        "@stryker-mutator/mocha-framework": "^2.4.0",
        "@stryker-mutator/mocha-runner": "^2.4.0",
        "@stryker-mutator/typescript": "^2.4.0",
        "@types/chai": "^4.2.5",
        "@types/chai-as-promised": "^7.1.2",
        "@types/express": "^4.17.6",
        "@types/mocha": "^5.2.7",
        "@types/node": "^12.12.12",
        "@types/q": "^1.5.2",
        "@types/request-promise": "^4.1.45",
        "@types/supertest": "^2.0.8",
        "@types/yamljs": "^0.2.30",
        "chai": "^4.2.0",
        "chai-as-promised": "^7.1.1",
        "cpx": "^1.5.0",
        "mocha": "^6.2.2",
        "mocha-junit-reporter": "^1.23.1",
        "npm-watch": "^0.6.0",
        "nyc": "^14.1.1",
        "sinon": "^7.5.0",
        "supertest": "^4.0.2",
        "ts-node": "^8.5.2",
        "ts-sinon": "^1.0.24",
        "typescript": "^3.7.2"
    }
}